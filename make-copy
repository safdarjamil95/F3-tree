.PHONY: all clean
.DEFAULT_GOAL := all

LIBS=-lrt -lm -lpthread
INCLUDES=-I./include
CFLAGS=-O0 -std=c++11 -g 

output = fbtree_concurrent fbtree_concurrent_mixed

all: main

main: src/fbtest.cpp src/hash.h
	g++ $(CFLAGS) -o fbtree_concurrent src/fbtest.cpp $(LIBS) -DCONCURRENT 
	g++ $(CFLAGS) -o fbtree_concurrent_mixed src/fbtest.cpp $(LIBS) -DCONCURRENT -DMIXED

clean: 
	rm $(output)
